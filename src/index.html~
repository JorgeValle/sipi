			<script>
		  function initMap() {
			var uluru = {lat: 12.1150, lng: -86.2362};
			var map = new google.maps.Map(document.getElementById('map'), {
			  zoom: 4,
			  center: uluru
			});
			var marker = new google.maps.Marker({
			  position: uluru,
			  map: map
			});
		  }
		</script>
